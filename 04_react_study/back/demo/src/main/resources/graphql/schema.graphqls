
type Mutation {
    createGraphql(writer: String!, title: String!, contents: String!): Graphql
    deleteBoard(id: Int!): GraphqlRemoveResponseDto
}

type GraphqlRemoveResponseDto {
    message: String!
}


type Graphql {
    message: String!
    writer: String!
    title: String!
    contents: String!
    id: ID!
}

type Query {
    fetchBoard(number: Int!): Board
    fetchBoards : [Board!]!
}

type Board {
    id: ID!
    writer: String!
    title: String!
    contents: String!
}