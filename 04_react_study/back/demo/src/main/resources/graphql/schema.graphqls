
type Mutation {
    createGraphql(writer: String!, title: String!, contents: String!): Graphql
}
type Graphql {
    message: String!
    writer: String!
    title: String!
    contents: String!
    id: ID!
}

type Query {
    fetchBoard(number: Int!): Board
}

type Board {
    id: ID!
    writer: String!
    title: String!
    contents: String!
}